{
    "name": "siemendev/checkout-gift-card-symfony-bridge",
    "description": "checkout gift card symfony bridge",
    "version": "0.1.0",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Siemendev\\Checkout\\GiftCard\\SymfonyBridge\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Patrick Siemen",
            "email": "post@patrick-siemen.de"
        }
    ],
    "require": {
        "php": ">=8.1",
        "siemendev/checkout-gift-card": "*",
        "siemendev/symfony-package-helper": "*",
        "symfony/framework-bundle": "^5.0|^6.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../checkout"
        },
        {
            "type": "path",
            "url": "../payment"
        },
        {
            "type": "path",
            "url": "../products"
        },
        {
            "type": "path",
            "url": "../package-helper"
        },
        {
            "type": "path",
            "url": "../gift-card"
        }
    ]
}
