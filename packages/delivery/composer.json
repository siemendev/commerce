{
    "name": "siemendev/checkout-delivery",
    "description": "checkout delivery implementation",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Siemendev\\Checkout\\Delivery\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Patrick Siemen",
            "email": "post@patrick-siemen.de"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "siemendev/checkout": "dev-main"
    },
