<a href="#" onclick="fill();">Fill with demo data?</a><br><br>
<input name="name" type="text" placeholder="Full name" value="{{ address.name }}"><br>
<input name="address_line1" type="text" placeholder="Address Line 1" value="{{ address.addressLine1 }}"><br>
<input name="address_line2" type="text" placeholder="Address Line 2" value="{{ address.addressLine2 }}"><br>
<input name="postal_code" type="text" placeholder="Postal code" value="{{ address.postalCode }}">
<input name="city" type="text" placeholder="City" value="{{ address.city }}"><br>
<input name="state" type="text" placeholder="State" value="{{ address.state }}"><br>
<select name="country">
    <option value="">Select country</option>
    <option value="DE" {% if 'DE' == address.countryCode %} selected{% endif %}>Germany</option>
    <option value="US"{% if 'US' == address.countryCode %} selected{% endif %}>USA</option>
</select><br>

<script>
    function fill() {
        document.querySelector('input[name="name"]').value = 'John Doe';
        document.querySelector('input[name="address_line1"]').value = 'Sesamstra√üe 1';
        document.querySelector('input[name="address_line2"]').value = 'Wohnung 69';
        document.querySelector('input[name="postal_code"]').value = '12345';
        document.querySelector('input[name="city"]').value = 'Berlin';
        document.querySelector('input[name="state"]').value = 'Berlin';
        document.querySelector('select[name="country"]').value = 'DE';
    }
</script>
